        c1_0.name,
        c1_0.parent_id,
        c1_0.slug,
        c1_0.updated_at 
    from
        categories c1_0 
    left join
        categories p1_0 
            on p1_0.id=c1_0.parent_id 
    where
        p1_0.id=? 
    order by
        c1_0.display_order
