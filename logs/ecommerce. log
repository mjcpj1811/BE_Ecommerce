        ua1_0.created_at,
        ua1_0.district,
        ua1_0.is_default,
        ua1_0.phone,
        ua1_0.recipient_name,
        ua1_0.user_id,
        ua1_0.ward 
    from
        user_addresses ua1_0 
    left join
        users u1_0 
            on u1_0.id=ua1_0.user_id 
    where
        u1_0.id=? 
    order by
        ua1_0.is_default desc,
        ua1_0.created_at desc
