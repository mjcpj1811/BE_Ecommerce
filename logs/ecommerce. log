    select
        s1_0.id,
        b1_0.id,
        b1_0.available_balance,
        b1_0.pending_balance,
        b1_0.shop_id,
        b1_0.total_withdrawn,
        b1_0.updated_at,
        s1_0.banner_url,
        s1_0.commission_rate,
        s1_0.created_at,
        s1_0.description,
        s1_0.logo_url,
        s1_0.name,
        s1_0.owner_id,
        s1_0.rating,
        s1_0.slug,
        s1_0.status,
        s1_0.total_products,
        s1_0.total_reviews,
        s1_0.total_sold,
        s1_0.updated_at 
    from
        shops s1_0 
    left join
        shop_balances b1_0 
            on s1_0.id=b1_0.shop_id 
